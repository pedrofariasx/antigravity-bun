# Server Configuration
PORT=3000

# API Key for protecting the proxy (optional)
# If set, clients must include "Authorization: Bearer <PROXY_API_KEY>" header
PROXY_API_KEY=

# Antigravity OAuth Credentials
# Get these by running the OAuth flow: visit /oauth/authorize
ANTIGRAVITY_ACCESS_TOKEN=
ANTIGRAVITY_REFRESH_TOKEN=
ANTIGRAVITY_EXPIRY_DATE=

# Optional: Override the default project ID (normally auto-configured)
ANTIGRAVITY_PROJECT_ID=

# Optional: Email associated with the Google account
ANTIGRAVITY_EMAIL=

# OAuth callback configuration (optional, defaults shown)
OAUTH_CALLBACK_PORT=51121
OAUTH_CALLBACK_PATH=/oauthcallback
